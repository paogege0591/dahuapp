.item{
  border-bottom: 1rpx solid #ccc;
  padding:20rpx 40rpx;
  clear: both;
  height: 80rpx;
}
.item-left{
  display: inline-block;
  width: 300rpx;
}
.type{
  font-weight: bold;
  font-size: 34rpx;
}
.time{
  font-size: 26rpx;
  color: #ccc;
}
.item-right{
  font-size: 32rpx;
  float:right;
  margin:auto;
  line-height: 80rpx;
}
.item-green{
  color:#00a300;
}
