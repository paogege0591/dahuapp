page {
  background: #f3f3f3;
}
.userinfo {
  background: linear-gradient(#114b70, #c9d4db);
  width: 100%;
  height: 300rpx;
  border-bottom-left-radius: 375rpx 50rpx;
  border-bottom-right-radius: 375rpx 50rpx;
}
.userContainer,
.userinfoContainer {
  width: 95%;
  height: 100%;
  margin: 0px auto;
}
.memberArea,
.userInterest,
.myOrder {
  background: #ffffff;
  border-radius: 10rpx;
  width: 100%;
}
.userInterest {
  height: 173rpx;
  margin-top: -82rpx;
  display: flex;
  /* padding: 0rpx 10rpx; */
}
.myOrder {
  height: 255rpx;
  margin: 30rpx 0px;
  /* padding: 0rpx 20rpx; */
  position: relative;
}
.myOrder button {
  width: 223rpx;
  font-size: 30rpx;
  top:88rpx;
  background: #52d3bb;
  margin: 0px auto;
}
.myOrder .button-hover {
  background: #2ea790;
}
.memberArea {
  height: 180rpx;
  margin: 30rpx 0px;
  display: flex;
  /* padding: 0rpx 20rpx; */
}
.Recommend {
  width: 100%;
}
.recommendImg {
  width: 100%;
  height: 100%;
  margin: 30rpx 0px;
}
.userinfoContainer {
  display: flex;
}
.headPic {
  flex: 2;
  padding: 25rpx 0px;
}
.picarea {
  float: left;
  border-radius: 75rpx;
  width: 130rpx;
  height: 130rpx;
  margin: 25rpx;
}
.picarea image {
  border-radius: 100rpx;
}
.headName {
  float: left;
}
.rememberIcon {
  width: 220rpx;
  height: 60rpx;
  line-height: 60rpx;
  background: #f8b551;
  margin: 20rpx 0rpx;
  border-radius: 80rpx;
  color: #ac7118;
  font-weight: 600;
  font-size: 30rpx
}
.rememberName {
  color: #fff;
  font-size: 34rpx;
  line-height: 50rpx;
  margin: 20rpx 0rpx;
}
.headSet {
  text-align: right;
  flex: 1;
  color: #fff;
  font-size: 35rpx;
  padding: 40rpx 40rpx;
}
/* .userInterest-card{flex: 1;text-align: center;padding:20rpx 0px;} */
.userInterest-card {
  flex: 1;
  text-align: center;
  padding: 0rpx 0px;
  height: 67%;
  margin-top: 15px;
}
.userInterest-card-leftborder {
  border-right: 1px solid #e5e5e5;
}
.userInterest-text {
  font-size: 22rpx;
  color: #6a6a6a;
}
.userInterest-num {
  font-size: 26rpx;
  color: #dc9f45;
  line-height: 90rpx;
}
.userInterest-img {
  height: 95rpx;
  width: 55%;
  margin: 0px auto;
}
.myOrder-tit {
  display: flex;
  width: 100%;
  line-height: 90rpx;
  border-bottom: 1px solid #e5e5e5;
  font-size: 27rpx;
}
.myOrder-container {
  width: 100%;
  height: 100%;
  display: block;
}
