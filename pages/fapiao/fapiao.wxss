page{
  background-color: #f7f7f7;
}
.foot-add{
  position: fixed;
  height: 80rpx;
  line-height: 80rpx;
  top: auto;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: #f55;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
  z-index: 999;
}
.fapiao-box{
  padding: 10rpx 0;
  font-size: 28rpx;
  margin-bottom: 80rpx;
}
.fapiao-item{
  margin: 20rpx 0;
  padding:0 30rpx;
  background-color: #fff;
}
.line1{
  height: 60rpx;
  padding: 10rpx;
  border-bottom: 1rpx solid #eee;
}
.fapiao-title{
  height: 40rpx;
  line-height: 40rpx;
}
.fapiao-taxnum{
  height: 20rpx;
  line-height: 20rpx;
  font-size: 26rpx;
  color: #999;
}
.line2{
  height: 80rpx;
  line-height: 80rpx;
}
.line2-left{
  display: inline-block;
}
.line2-right{
  float: right;
  color: #999;
}