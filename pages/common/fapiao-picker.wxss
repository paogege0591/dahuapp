.fapiao-area{
  position: fixed;
  bottom: 0;
  top: 0;
  left: 0;
  right: 0;
  background-color: #fff;
}
.fapiao-set{
  position: fixed;
  height: 80rpx;
  line-height: 80rpx;
  top: 0;
  left: 0;
  right: 0;
  bottom: auto;
  background-color: rgb(90, 105, 240);
  color: #fff;
  font-size: 28rpx;
  text-align: center;
  z-index: 999;

}
.fapiaofoot-add{
  position: fixed;
  height: 80rpx;
  line-height: 80rpx;
  top: auto;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: #f55;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
  z-index: 999;
}
.fapiao-box{
  padding: 10rpx 0;
  font-size: 28rpx;
  margin-bottom: 80rpx;
  margin-top: 80rpx;
}
.fapiao-item{
  margin: 20rpx 0;
  padding:0 30rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #eee;
  display: flex;
  flex-direction: row;
}
.fapiaoline1{
  width: 600rpx;
  height: 60rpx;
  padding: 10rpx;
  
}
.fapiao-title{
  height: 40rpx;
  line-height: 40rpx;
}
.fapiao-taxnum{
  height: 20rpx;
  line-height: 20rpx;
  font-size: 26rpx;
  color: #999;
}
.fapiaoline2{
  width: 80rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  background-color: #f55;
  color: #fff;
  margin: auto;
  border-radius: 40rpx;
  
}