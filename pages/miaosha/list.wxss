page{
  background-color: #f7f7f7;
  font-size: 30rpx;
}
.ms_container{
  width: 700rpx;
  margin: 0 25rpx;
}
.ms_item{
  width: 680rpx;
  margin: 10rpx 0;
  padding:0 10rpx;
  background-color: #fff;
  height: 160rpx;
  display: flex;
  flex-direction: column;
}
.ms_item_line1{
  display: flex;
  flex-direction: row;
  height: 80rpx;
  line-height: 40rpx;
  padding: auto;
}
.ms_item_line2{
  height: 80rpx;
  line-height: 40rpx;
}
.ms_item_title{
  width: 200rpx;
  font-weight: bold;
  color: #F55;
}
.ms_item_comment{
  color: #999;
  overflow: hidden;
}
.ms_item_starttime{
  color: #54a4fe;
}
.ms_item_endtime{
  color: #ff9e40;
}