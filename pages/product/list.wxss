.search-box {
  width: 90%;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 30rpx;
  background-color: #efefef;
  color: #9f9f9f;
  font-size: 30rpx;
  display: inline-block;
}

.holder {
  color: #9f9f9f;
  font-size: 30rpx;
  line-height: 60rpx;
  margin-left: 30rpx;
}

.search-icon {
  width: 10%;
  font-size: 50rpx;
  color: #9f9f9f;
}

.search-font {
  width: 80%;
  display: inline-block;
  margin-left: 30rpx;
}

.liebiao-icon {
  height: 60rpx;
  font-size: 44rpx;
  line-height: 60rpx;
  width: 8%;
  margin-left: 2%;
  color: #bbb;
}

.pro-area {
  background-color: #f7f7f7;
  padding: 15rpx;
}

.pro-item {
  margin: 15rpx;
  display: inline-block;
  width: 330rpx;
  background-color: #fff;
}

.pro-img {
  width: 330rpx;
  height: 330rpx;
}

.pro-name {
  line-height: 40rpx;
  font-size: 30rpx;
  height: 120rpx;
}

.pro-bottom-left {
  display: inline-block;
  color: #f55;
  font-size: 30rpx;
  height: 40rpx;
  line-height: 40rpx;
}

.pro-bottom-right {
  color: #f55;
  float: right;
  margin-right: 20rpx;
}

.icon-gouwuche {
  font-size: 40rpx;
}

.clear {
  clear: both;
  overflow: hidden;
}

.tabTit {
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: solid 2rpx #eee; /* position:fixed; */
  top: 60rpx;
  width: 100%;
  z-index: 1;
  background: #fff;
  font-size: 30rpx;
  text-align: center;
}

.tabTit .active {
  color: #e64340;
  background: #fff;
}

.tabTit .active image {
  transform: rotate(180deg);
  background: none;
}

.tabTit image {
  width: 20rpx;
  height: 13rpx;
  vertical-align: middle;
  /* margin-left: 10rpx; */
  margin-left:195rpx;
  margin-top:-50rpx;
}

.tabLayer {
  box-shadow: 0 10rpx 10rpx rgba(0, 0, 0, 0.15);
  width: 100%;
  overflow: hidden;
  top: 153rpx;
  z-index: 1;
  background: #fff;
  padding-bottom: 40rpx;
  border-bottom: solid 1px #eee;
  text-align: center;
}

.tabLayer text {
  width: 210rpx;
  height: 60rpx;
  line-height: 60rpx;
  float: left;
  border: solid 2rpx #eee;
  margin: 20rpx 0 0 26rpx;
  font-size: 15px;
}

.tabLayer .active {
  color: #e64340;
  border-color: #e64340;
}

.flex1 {
  flex: 1;
  overflow: hidden;
  display: block;
}

/*弹性盒模型*/

.box {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}

navigator {
  display: inline;
}